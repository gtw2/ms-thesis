@techreport{maggos_update_2015,
	title = {Update on {Electrochemical} {Mass} {Balance} {Modeling} for {Safeguards}},
	abstract = {Pyrochemical recycling of used nuclear has recently been an area of focus for the Material Protection, Accounting, and Control Technologies (MPACT) Campaign as this technology becomes more advanced. Therefore, chemical process models that simulate the operations within an electrochemical used fuel treatment facility are being developed to support the design and implementation of electrochemical process monitoring and material accountability systems. These codes predict the evolving material distribution of uranium and transuranics (TRU), as well as significant fission products and process components, in an electrochemical facility and in the electrorefiner in particular. Calculation of the in-process inventory serves as part of an integrated approach to safeguards by informing the design and selection of measurement techniques, sensor arrangement and measurement frequency, and the development of specialized analysis tools. The ability to predict material inventory may also be necessary when direct measurement is not practical during process operations, or when a fully integrated set of processes is not available.},
	language = {en},
	number = {ANL/FCRD-MPACT/2015-9-2},
	author = {Maggos, Laura E and Pereira, Candido and Caputo, John A F and Copple, Jacqueline M},
	month = sep,
