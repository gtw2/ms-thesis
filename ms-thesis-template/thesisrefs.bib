@article{park_study_2008,
	title = {Study on a {Salt} {Evaporation} of the {Uranium} {Deposits} from an {Electrorefiner}},
	url = {http://www.iaea.org/inis/collection/NCLCollectionStore/_Public/40/003/40003954.pdf},
	abstract = {Uranium metal is electrodeposited onto a solid cathode during the electrorefining process.
Uranium deposits from an electrorefiner contain about 30{\textasciitilde}40 wt\% salts. In order to recover pure uranium
and transform it into metal ingots, the salts have to be removed. A salt distiller is adapted for a salt
evaporation. A batch operation for the salt removal is carried out by a heating and vacuum evaporation. It
is operated at 700 {\textasciitilde} 1000 oC and less than 1 Torr, respectively. The behaviors of the salt evaporations
were investigated by focusing on the effects of the vacuum pressure and the holding temperature on the salt
distillation. The salt removal efficiencies were obtained with regards to the operational conditions. The
Hertz-Langmuir relation was applied to the experimental results of the salt evaporations. The effective
evaporation coefficients of the relation were obtained with regards to the operational conditions. The lower
the vacuum pressure and the higher the holding temperature were, the higher the removal efficiencies of
the salts were.},
	urldate = {2018-04-27},
	journal = {ATALANTE},
	author = {Park, Sung Bin and Cho, Dong Wook and Oh, Gyu Hwan and Lee, Jong Hyeon and Hwang, Sung Chan and Kang, Young Ho and Lee, Han Soo and Kim, Eung Ho and Park, Seong Won},
	month = may,
	year = {2008}
}

